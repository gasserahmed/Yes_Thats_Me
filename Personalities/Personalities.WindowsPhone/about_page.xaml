    <Page.BottomAppBar>
        <CommandBar Background="#FF0095DA">
            <AppBarButton Icon="LikeDislike" Label="rate it" Click="AppBarButton_Click"/>
            </CommandBar>
    </Page.BottomAppBar>
    <Page.Background>
        <ImageBrush ImageSource="Assets/About background.png" Stretch="UniformToFill"/>
    </Page.Background>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="200"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <TextBlock x:Name="textBlock" Grid.Row="0" Text="About" FontSize="25" Margin="12,0,0,0" Foreground="#FF0095DA" RenderTransformOrigin="0.5,0.5">
            <TextBlock.RenderTransform>
                <CompositeTransform/>
        	</TextBlock.RenderTransform>
        </TextBlock>
        <Grid Grid.Row="1">
            <Grid VerticalAlignment="Center">
                <Grid.RowDefinitions>
                    <RowDefinition Height="auto"/>
                    <RowDefinition Height="auto"/>
                    <RowDefinition Height="auto"/>
                </Grid.RowDefinitions>
                <!--app name-->
                <TextBlock Grid.Row="0" Text="Yes,That's Me" FontSize="40" Margin="12,0,0,0" HorizontalAlignment="Left"/>
                <!--developer name-->
                <TextBlock Grid.Row="1" Text="By: Gasser Montaser Saleh" FontSize="20" FontWeight="ExtraLight" Margin="12,0,0,0" HorizontalAlignment="Left" Foreground="#FF0095DA" />
                <!--app version-->
                <TextBlock Grid.Row="2" Text="Version 1.0.0" FontSize="15" FontWeight="ExtraLight" Margin="12,0,0,0"/>
            </Grid>
            <Image Width="100" Height="100" VerticalAlignment="Top" HorizontalAlignment="Right" Source="Assets/Square71x71Logo.png" Margin="10"/>
        </Grid>
        <!--app description-->
        <TextBlock Grid.Row="2" Text="'Yes,That's Me' is an application that helps you in knowing your own personality in an interesting way." FontSize="20" Margin="12" TextAlignment="Left" TextWrapping="Wrap" FontWeight="ExtraLight"/>
        <!--universty name-->
        <TextBlock Grid.Row="3" Text="Alexandria University" FontSize="20" HorizontalAlignment="Left" FontWeight="ExtraLight" Margin="12,0,0,0"/>
        <TextBlock Grid.Row="4" Text="Assembly Line 5-1 Program - Trainee" FontSize="15" HorizontalAlignment="Left"  FontWeight="ExtraLight" Margin="12,0,0,0" Width="330" Foreground="#FF0095DA" />
        <StackPanel Grid.Row="5" Orientation="Horizontal" Margin="30" >
            <Image x:Name="image" Height="50" Source="Assets/About/Logo Icon2.png" RenderTransformOrigin="0.5,0.5">
                <Image.RenderTransform>
                    <CompositeTransform/>
                </Image.RenderTransform>
            </Image>
            <Image x:Name="image1" Height="50" Source="Assets/About/Logo Icon 1.png" RenderTransformOrigin="0.5,0.5">
                <Image.RenderTransform>
                    <CompositeTransform/>
                </Image.RenderTransform>
            </Image>
            <Image x:Name="image2" Source="Assets/About/Assembly 5-1.png" Height="50" RenderTransformOrigin="0.5,0.5">
                <Image.RenderTransform>
                    <CompositeTransform/>
                </Image.RenderTransform>
            </Image>
        </StackPanel>
        <TextBlock Grid.Row="6" Foreground="#FF0095DA" TextWrapping="Wrap" Text="Your Future goes here;" FontSize="24" FontWeight="ExtraLight"/>
        <Grid Grid.Row="7">
            <Grid.RowDefinitions>
                <RowDefinition Height="auto"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="auto"/>
                <ColumnDefinition Width="auto"/>
                <ColumnDefinition Width="auto"/>
                <ColumnDefinition Width="auto"/>
            </Grid.ColumnDefinitions>
            <Image Grid.Row="0" Grid.Column="0" Source="Assets/about/FBL.png" Height="30" Margin="5" Tapped="Image_Tapped"/>
            <Image Grid.Row="0" Grid.Column="2" Source="Assets/about/TL.png" Height="30" Margin="5" Tapped="Image_Tapped_1"/>
            <TextBlock Grid.Row="0" Grid.Column="1" Text="/EGAppFactory" FontSize="13" Foreground="White" VerticalAlignment="Center" Tapped="TextBlock_Tapped_1"/>
            <TextBlock Grid.Row="0" Grid.Column="4" Text="#EgAppFactory" FontSize="13" Foreground="White" VerticalAlignment="Center" Tapped="TextBlock_Tapped_2"/>
        </Grid>
    
    </Grid>
</Page>
